network:
  version: 2
  renderer: networkd
  ethernets:
    {{ interface_name }}:
      dhcp4: no
      addresses:
        - {{ ipv4_address }}
        - {{ ipv6_address }}
      routes:
        - to: default
          via: {{ gateway_ip }}
      nameservers:
        addresses:
          - {{ dns_server_ip }}
